{
  "[astro]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "cSpell.userWords": [],
  "colorize.languages": [
    "typescript",
    "typescriptreact"
  ],
  "editor.colorDecoratorsActivatedOn": "click",
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "'MonaspiceNe Nerd Font Mono', monospace, 'Noto Sans CJK SC'",
  "editor.fontLigatures": "'calt' off, 'liga', 'ss01', 'cv11'",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.lineHeight": 24,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.useTabStops": false,
  "errorLens.decorations": {
    "errorMessage": {
      "color": "#bf616a",
      "fontWeight": "bold"
    },
    "hintMessage": {
      "color": "#a3be8c",
      "fontStyleItalic": "true"
    },
    "infoMessage": {
      "color": "#88c0d0",
      "fontStyleItalic": "true"
    },
    "warningMessage": {
      "color": "#d08770"
    }
  },
  "eslint.codeActionsOnSave.rules": null,
  "eslint.format.enable": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "astro"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "astro"
  ],
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "security.promptForLocalFileProtocolHandling": false,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.stickyScroll.enabled": false,
  "vim.easymotion": true,
  "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
  "vim.easymotionMarkerForegroundColorOneChar": "#8fbcbb",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#5e81ac",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#81a1c1",
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "<C-k>"
      ],
      "commands": [
        "selectPrevSuggestion"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "H"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "L"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "M"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<C-h>"
      ],
      "commands": [
        "workbench.action.navigateLeft"
      ]
    },
    {
      "before": [
        "<C-l>"
      ],
      "commands": [
        "workbench.action.navigateRight"
      ]
    },
    {
      "before": [
        "<C-j>"
      ],
      "commands": [
        "workbench.action.navigateDown"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "commands": [
        "workbench.action.navigateUp"
      ]
    },
    {
      "before": [
        "<leader>",
        "r"
      ],
      "commands": [
        "editor.action.rename"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<Esc>"
      ],
      "when": [
        "editorFocus"
      ],
      "commands": [
        ":nohl"
      ],
      "silent": true
    }
  ],
  "vim.searchHighlightColor": "#f6ff0080",
  "vim.searchHighlightTextColor": "#fff",
  "vim.surround": true,
  "window.commandCenter": false,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.customTitleBarVisibility": "never",
  "window.nativeTabs": true,
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Nord",
  "workbench.editor.customLabels.patterns": {
    "**/index.*": "${dirname} .../${dirname(1)}",
    "**/{page,layout,template,route,actions,hooks,components,utils,types}.{js,ts,jsx,tsx,vue}": "${dirname}/${filename}.${extname} .../${dirname(1)}"
  },
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "workbench.iconTheme": "file-icons-colourless",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none"
}